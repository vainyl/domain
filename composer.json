{
    "name": "vainyl/domain",
    "type": "shared-package",
    "authors": [
        {
            "name": "Taras Girnyk",
            "email": "taras.p.gyrnik@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Vainyl\\Domain\\": "src/"
        }
    },
    "require": {
        "php": ">=7.1",
        "vainyl/core": "0.2.x-dev",
        "vainyl/event": "0.2.x-dev",
        "vainyl/operation": "0.2.x-dev",
        "vainyl/time": "0.2.x-dev"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://satis.vainyl.com:444"
        }
    ],
    "suggest" : {
        "vainyl/doctrine-orm-bridge" : "Doctrine relational entities with domain support",
        "vainyl/doctrine-odm-bridge" : "Doctrine document entities with domain support"
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}